body {
    letter-spacing: normal;
}

h1,
h2,
h3,
h4,
h5,
.h0,
.h1,
.h2,
.h3,
.h4,
.h5 {
    font-family: Hvdtrial Pluto, Arial, sans-serif;
    letter-spacing: normal;
}

header.sec-section-3 {
    position: sticky;
    top: 0px;
}

.sec-link-2[aria-current="page"] {
    font-family: Hvdtrial Pluto, Arial, sans-serif;
    font-weight: 700;
}

.sec-link-block-1 {
    display: inline-flex;
}

.sec-icon-1 {
    display: inline-block;

    & svg {
        height: 100%;
        width: 100%;
    }
}

.sec-link-block-1 {
    position: relative;
}

.cart-count-bubble {
    left: 60%;
    top: -10%;
}

.cart-notification {
    background-color: white;
}

.cart__ctas button[type="submit"],
.cart-drawer__empty-content a[class="button"] {
    color: #fff;
    background-color: #171717;
    border-radius: 8em;
    padding: .71em 4em;
    font-family: Hvdtrial Pluto, Arial, sans-serif;
    font-size: 1em;
    font-weight: 700;
    line-height: 1.3em;
    border: none;

    &::before,
    &::after,
    &:hover::after {
        box-shadow: none;
    }
}

.cart-item__name {
    color: inherit;
    text-decoration: none;
}

.cart-notification-product{
    gap: 1em;

    & > div{
        margin-right: 0px;
        margin-top: 0px;
        border: none;
        border-radius: 0.57em;
        overflow: hidden;

        &:first-child{
            flex: 1;

            & img{
                object-fit: cover;
                object-position: center;
            }
        }
        &:last-child{
            flex: 4;
        }
    }
}

.drawer__inner {
    box-shadow: -2px 0px 5px 0px #88888814;
}

.buh-imput-1 {
    color: white;
    outline: none !important;
    box-shadow: none !important;
}

.buh-imput-1:focus::placeholder {
    color: #fff !important;
    opacity: 0.5 !important;
}

.buh-icon-1 {
    height: 2em;
    display: block;
    width: auto;
    color: white;

    & svg {
        height: 100%;
        width: 100%;
    }

    &.full {
        aspect-ratio: 1/1;
        background: white;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: #171717;

        & svg {
            height: 55%;
            width: 55%;
        }
    }
}

.leo-text-2.price {
    color: #4d4d4d;
    font-size: 1.11em;
    letter-spacing: normal;
}

.button, .button:hover, .button:focus {
    &::after,
    &::before,
    &:hover::after,
    &:focus-visible {
        box-shadow: none;
    }
}

.leo-button-1.button {
    background-color: #1a2b47;
    border: 1.05px solid #1a2b47;
    min-width: auto;
    min-height: auto;
    text-transform: uppercase;
}

.leo-button-1 .loading__spinner {
    color: white;

    & .path {
        stroke: white;
    }
}

.leo-wrap-7 {
    display: grid;
    grid-template-columns: 1fr 1fr;
}

.boton-estetico,
.divancla {
    width: 100%;
    height: 100%;
    aspect-ratio: 1 / 1;
}

.leo-wrap-4 {
    justify-content: space-between;
}

.leo-wrap-44 {
    display: grid;
    grid-column-gap: 1.42em;
    grid-row-gap: 1.42em;
    width: 100%;
}

.leo-wrap-5 {
    display: flex;
    justify-content: center;
}

.slick-slide>div {
    padding: 0px 1.175em;
}

.leo-wrap-1 {
    position: relative;
}

.leo-wrap-2 {
    overflow: hidden;
}

.leo-wrap-absolute-1 {
    inset: 50% -2%;
}

.aab-wrap-2.active .aab-text-1 {
    font-weight: bold;
}

.aab-grid-1.loading {
    opacity: 0.3;
}

.aab-wrap-2.hide {
    display: none;
}

.sat-rich-text-1.w-richtext {
    & ul, & ol {
        grid-column-gap: 1.14em;
        grid-row-gap: 1.14em;
        flex-flow: column;
        display: flex;
        padding-left: 0px;

        & li {
            background-color: #f5f7f9;
            border-radius: .57em;
            padding: 1.35em 1.71em 1.35em 5.77em;
            list-style: none;
            background-image: url("{{ 'otw-icon-check-fill.png' | asset_url }}");
            background-size: 1.9em 1.9em;
            background-position: 1.71em 1.35em;
            background-repeat: no-repeat;

        }
    }
}

.sat-rich-text-1.w-richtext{
    & p {
        line-height: 1.6em;
    }

}

[class*="-paragraph-1"] {
    & p {
        line-height: inherit;
        font-size: inherit;
        font-weight: inherit;
    }
}


input, textarea {
    &::after,
    &::before,
    &:hover::after,
    &:focus-visible {
        box-shadow: none;
        outline: none;
    }
}

.w-form-done, .w-form-fail {
    display: block !important;
}

#cart-notification-button{
    color: #171717;
    text-align: center;
    background-color: #fff;
    border: 1.05px solid #1a2b47;
    border-radius: 8em;
    flex: 1;
    align-self: center;
    padding: .84em 1.59em;
    font-size: 1.04em;
    font-weight: 700;
    line-height: 1.3em;
}

#cart-notification-form button{
    color: #fff;
    text-align: center;
    border-radius: 8em;
    flex: 1;
    align-self: center;
    padding: .84em 1em;
    font-size: 1.04em;
    font-weight: 700;
    line-height: 1.3em;
    background-color: #1a2b47;
    border: 1.05px solid #1a2b47;
    min-width: auto;
    min-height: auto;
}

.def-icons-1, .rom-icons-1{
    & svg {
        height: 100%;
        width: 100%;
    }

    & path{
        fill: #ec50252e;
    }
}

.def-wrap-6-1{
    display: block;
    height: auto;
    max-height: 0px;
    transition: max-height 0.5s ease-in;
    overflow: hidden;
    margin-bottom: 0px;

    & > div{
        padding-bottom: 1.14em;
    }
}
.def-wrap-5-1.open ~ .def-wrap-6-1{
    max-height: 300px;
}

[data-otw-carousel_action]{
    cursor: pointer;
}
.leo-heading-2{
    text-align: center;
}

[data-otw-carousel] {
    --translateX: -100%;
    --transitionDuration: 300ms;
}

[data-otw-carousel_container] {
    grid-column-gap: 0px;
    transform: translateX(-1em);
}

[data-otw-carousel_wrapper] {
    overflow-x: hidden;
    width: calc((var(--projectsLength) + 1) * var(--cardWidth));
    flex-shrink: 0;
}

[data-otw-carousel_container] > article, [data-otw-carousel_wrapper] > a, [data-otw-carousel_wrapper] > a.w--current {
    flex-shrink: 0;
    flex-basis: var(--cardWidth);
    width: var(--cardWidth);
    transition: transform var(--transitionDuration);
    transform: translateX(var(--translateX));
    padding: 0px 1em;
    order: 20;
    

    & > div {
        height: 100%;
    }
}

.clone {
    order: 30 !important;

    &:first-child {
        order: 0 !important;
    }
}

.def-wrap-6-3{
    position: relative;

    &.def-wrap-6-2{
        display: none;
    }

    .svg-wrapper{
        width: 100%;
    }

    & > button[data-otw-carousel_action="left"]{
        position: absolute;
        left: 0%;
        height: calc(var(--cardWidth) - 1.56em);
        bottom: 0px;
        background-color: #f5fbfd;
        padding: 0px;
        width: 1.5em;

        & span{
            transform: rotate(90deg);
        }
    }

    & > button[data-otw-carousel_action="right"]{
        position: absolute;
        right: 0%;
        height: calc(var(--cardWidth) - 1.56em);
        bottom: 0px;
        background-color: #f5fbfd;
        padding: 0px;
        width: 1.5em;

        & span{
            transform: rotate(-90deg);
        }
    }
}

.def-wrap-4{
    width: 50%;
    overflow: hidden;

    & [data-otw-carousel_container]{
        transform: translateX(0px);
    }

    & [data-otw-carousel_wrapper]{
        transform: translate(2em);
    }

    & .def-tabs-menu-1 {
        grid-column-gap: 0px;
        grid-row-gap: 0px;
    }

    & .def-tab-link-1, & .def-tab-link-1.w--current{
        padding: 0 0.78em;
    }
}

[data-otw-items="1"], [data-otw-items="2"], [data-otw-items="3"], [data-otw-items="4"]{
    & > button{
        display: none;
    }
}

.mol-grid-1 {
    grid-template-rows: none;
}

.mol-wrap-4{
    grid-area: 1 / 2 / span 3 / 2;
}

.quantity{
    background-color: white;
    border-radius: 8em;
    overflow: hidden;
    border: solid 1px black;
    width: auto;

    &::after, &::before, &:hover::after, &:focus-visible {
        box-shadow: none;
        outline: none;
    }
}

.quantity__button:focus-visible, .quantity__input:focus-visible {
    background-color: transparent;
    z-index: 2;
}

div.product__media-wrapper, div.product__info-wrapper{
    flex: 0 0 50%;
}

div.def-container-1 div.product__info-wrapper{
    max-width: none;
    width: 50%;
}

.def-wrap-1-1{
    & > a{
        display: none;
    } 
}

div.def-container-1{
    & .product__info-container > * + * {
        margin: 0;
    }

    & label{
        margin-bottom: 0px;
    }

    & div.product__media > img {
        border-radius: .57em;
    }

    & .global-media-settings, & div.global-media-settings{
        border: none;
        background-color: transparent;
        border-radius: .57em;
    }

    & .thumbnail[aria-current]{
        border: none;
        box-shadow: none;
    }

    & .product__info-container{
        display: flex;
        flex-direction: column;
        gap: 1.21em;
    }

    & .def-heading-1{
        margin-top: 0.2em;
    }

    & .product.grid{
        gap: 3.57em;
        display: grid;
        grid-template-columns: 1fr 1fr;
    }

    & div.grid__item{
        max-width: none;
        width: 100%;
    }

    & .product-form__input--pill input[type='radio'] + label{
        text-transform: capitalize;
        background-color: #fff;
        border: 1px solid #1717174b;
        border-radius: 8em;
        padding: .71em 1.14em;
        color:#171717;
        font-size: 1.07em;
        font-weight: 400;
        line-height: 1.3em;
    }

    & .product-form__input--pill input[type='radio']:checked + label{
        text-transform: capitalize;
        background-color: #fff;
        border: 1px solid #171717;
        border-radius: 8em;
        padding: .71em 1.14em;
        color:#171717;
        font-size: 1.07em;
        font-weight: 400;
        line-height: 1.3em;
    }

    & .def-wrap-7{
        margin-top: 4em;
    }

    & .def-toggle-2{
        padding: 0px;
    }

    & .def-text-1-3{
        width: 100%;
        margin-bottom: 0px;
        padding: .82em 2.28em .82em 1.14em;
        cursor: pointer;
        font-weight: normal;

        & .def-span-2{
            font-weight: normal;
        }

        & input{
            display: none;
        }
    }

    & .def-icon-6{
        display: none;
    }

    & .def-icon-5-1.selected .def-icon-6{
        display: block;
    }

    & .def-wrap-2-6 input {
        display: none;

        &:checked + .def-icon-5 {
            background-color: #f5f7f9;
            font-weight: bold;
            display: flex;
            padding: 3px;

            &::after{
                content: ' ';
                width: 100%;
                height: 100%;
                background-color: #171717;
                border-radius: 50%;
            }
        }
    }

    & .shopify_subscriptions_app_container{
        display: none !important
    }

    & .def-text-1-2{
        cursor: pointer;
    }

    & div.def-grid-1{
        margin: 1.64em 0px;
    }

    & .product-form__input{
        flex: 0 0 15%;
        margin-bottom: 0px;
    }

    & .product__info-container .product-form{
        margin: 0px;
    }

    & .def-wrap-1-7{
        margin: 0.5em 0px;
    }

    & .def-wrap-1-7 > div:last-child{
        flex: 1;
    }

    & .product-form__buttons{
        max-width: none;

        & button {
            grid-column-gap: 2em;
            grid-row-gap: 2em;
            background-color: #1a2b47;
            border-radius: 8em;
            flex: 1;
            justify-content: center;
            align-items: center;
            padding: .82em 2.07em;
            text-decoration: none;
            display: flex;
            margin-bottom: 0px;
            text-transform: uppercase;
            font-weight: 700;
            font-size: 1em;
            background-image: url("{{ 'otw-vector-5.svg' | asset_url }}");
            background-repeat: no-repeat;
            background-size: auto;
            background-position: 95% center;

        
        }
    }

    & .product-form__input{
        min-width: auto;
    }

    & .quantity__button{
        width: 1.5em;
    }

    & .def-span-1{
        display: none;
        &.show{
            display: inline;
        }

        & i {
            position: absolute;
            height: 2px;
            background-color: #ffffffb3;
            width: 80%;
            transform: rotate(10deg);
            top: 39%;
            left: 19%;
        }
    }
}


button.thumbnail[aria-current]:focus:not(:focus-visible){
    outline: none;
    box-shadow: none;
    border: none;
}

.ext-section-1, .mol-section-1, .rom-section-2{
    padding-top: 0px;
    padding-bottom: 0px;

    & > div{
        padding-top: 5em;
        padding-bottom: 5em;
    }
}

.def-list-2{
    height: auto;
    max-height: 0px;
    overflow: hidden;
    transition: max-height 0.5s ease-in;
}

div.def-container-1 .variant_selects{
    display: flex;
    flex-direction: column;
    gap: 1.71em;
    margin-bottom: 1.64em;
}

.cart__warnings > a {
    color: #fff;
    background-color: #171717;
    border-radius: 8em;
    padding: .71em 4em;
    font-size: 1em;
    font-weight: 700;
    line-height: 1.3em;
    text-transform: uppercase;
    letter-spacing: normal;
}

.button{
    min-height: auto;
}

.cart-item__media > div{
    border-radius: .59em;
    overflow: hidden !important;
}

.leo-container-1 .card .card__inner, .leo-container-1 .card.card--card{
    border-radius: .57em;
    overflow: hidden !important;
}

.axe-wrap-1-1, .axe-wrap-1-7{
    background-color: white;
    border-radius: 50%;
}

.axe-grid-1 > a:first-child{
    grid-area: 1 / 1 / span 1 / span 3;
    position: relative;
    border-radius: .85em;
    justify-content: flex-start;
    align-items: flex-start;
    padding: 13.28em 2.85em 2.85em;
    text-decoration: none;
    display: flex;
    margin-bottom: 3.49em;

    .axe-wrap-4{
        position: absolute;
        height: 100%;
        inset: 0;
    }

    .axe-image-1{
        height: 100%;
        width: 100%;
        object-fit: cover;
        object-position: center;
    }

    .axe-wrap-5{
        position: relative;
        width: 45em;
    }

    .axe-heading-1, .axe-text-2, .axe-text-3{
        color: white;
    }

    .axe-heading-1{
        margin-top: 0;
        margin-bottom: 0;
        font-size: 2.57em;
        font-weight: 500;
        line-height: 1.1em;
    }

    .axe-text-2{
        font-size: 1.14em;
        font-weight: 500;
        line-height: 1.3em;
    }

    .axe-text-1{
        overflow-wrap: anywhere;
        background-color: #00a7d1;
        border-radius: .42em;
        padding: .28em .71em;
        font-size: 1em;
        font-weight: 500;
        line-height: 1.3em;
    }
}

.axe-wrap-6, .axe-wrap-6-1{
    display: flex;
    flex-wrap: wrap;
    gap: 1em;
}

.axe-text-1-2{
    font-family: Hvdtrial Pluto,Arial,sans-serif;
}

.axe-wrap-3{
    opacity: 0;
    transition: opacity 0.5s ease-in;
    animation: fadeIn 0.5s forwards;
}

@keyframes fadeIn {
    to {
        opacity: 1;
    }
}


@media screen and (max-width: 1540px) {
    .def-wrap-4 [data-otw-carousel_wrapper] {
        transform: translate(1em);
    }
}

@media screen and (min-width: 1440px) {
    .boton-estetico {
        width: 2.5em;
        height: 2.5em;
    }
}

@media screen and (min-width: 990px) {
    .cart-notification-wrapper:is(.page-width) > .cart-notification {
        right: -10rem;
    }

    div.def-container-1 .product--large:not(.product--no-media) .product__media-wrapper,
    div.def-container-1 .product--large:not(.product--no-media) .product__info-wrapper 
    {
        max-width: none;
        width: 100%;
    }

    div.def-container-1 .product--large:not(.product--no-media) .product__info-wrapper{
        padding-left: 0px;
    }
}

@media screen and (max-width: 991px) {
    .tec-section-1 {
        padding-bottom: 0px;
    }

    .tec-wrap-2 {
        padding-bottom: 5em;
    }

    .tec-wrap-3 {
        align-self: self-end;
    }

    .buh-wrap-1-2 {
        padding-top: .92em;
        padding-bottom: .92em;
    }

    .leo-wrap-7 {
        grid-template-columns: 1fr;
    }

    .leo-section-1 {
        padding-right: 0px;
    }

    .leo-wrap-absolute-1 {
        right: 1%;
    }
}

@media screen and (min-width: 750px) {
    section.product__info-container {
        max-width: none;
    }
}

@media screen and (max-width: 767px) {
    .tec-wrap-2 {
        padding-bottom: 0px;
    }

    .leo-section-1 {
        padding-right: 0px;
    }

    .leo-wrap-4 {
        width: 100%;
    }

    .def-wrap-6-3.def-wrap-6-2{
        display: block;
    }

    [data-otw-carousel_container], [data-otw-carousel_wrapper] {
        grid-column-gap: 0px;
        transform: translate(0px);
        overflow: hidden;
    }
    .def-tabs-menu-1.w-tab-menu{
        transform: translate(2em);
    }

    div.def-container-1{
        & .product.grid{
            grid-template-columns: 1fr;
        }
    }

    div.def-container-1 div.grid__item{
        overflow: hidden;
    }

    .skip-to-content-link{
        display: none;
    }
}

@media screen and (max-width: 479px) {
    .buh-icon-1 {
        height: 3em;
    }

    .buh-wrap-1-2 {
        padding-top: 1.1em;
        padding-bottom: 1.1em;
    }

    .sat-rich-text-1.w-richtext {
        & ul {

            & li {
                padding-left: 4.5em;
                background-size: 1.5em 1.5em;
                background-position: 1.3em 1.6em;

            }
        }
    }


}